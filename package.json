    "@vue/test-utils": "1.0.0-beta.29",
    "jest": "24.8.0",
    "node-eval": "2.0.0",
    "vue": "2.6.10",
    "vue-jest": "4.0.0-beta.2",
    "vue-template-compiler": "2.6.10",
