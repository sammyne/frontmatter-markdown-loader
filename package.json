    "frontmatter",
    "vue",
    "loader"
  ],
  "author": "Sammy Li <xiangminli@alumni.sjtu.edu.cn>",
  "contributors": [
    "Kengo Hamasaki <k.hamasaki@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sammyne/frontmatter-markdown-loader/issues"
  },
  "homepage": "https://github.com/sammyne/frontmatter-markdown-loader#readme",
  "dependencies": {
    "gray-matter": "^4.0.2",
    "loader-utils": "^1.2.3",
    "markdown-it": "^8.4.2"
  },
  "optionalDependencies": {
    "vue-template-compiler": "^2.5.0",
    "vue-template-es2015-compiler": "^1.6.0"
  },
  "devDependencies": {
